{
  "name": "d-source-code-react-js-app-furniture",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run build && vite preview",
    "lint": "eslint --ext .ts,.tsx src --color",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "analyze": "npm run lint && tsc && vite build --mode=analyze && source-map-explorer 'dist/assets/*.js'",
    "verify-commit": "verify-commit-msg",
    "postinstall": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.0",
    "@ant-design/static-style-extract": "^1.0.2",
    "@splidejs/react-splide": "^0.7.12",
    "ahooks": "^3.8.0",
    "antd": "^5.19.1",
    "aos": "^2.3.4",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "csvgtocss": "^0.0.10",
    "esno": "^4.7.0",
    "firebase": "^10.12.4",
    "framer-motion": "^11.2.13",
    "jotai": "^2.8.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.1",
    "slugify": "^1.6.6",
    "sonner": "^1.5.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.18",
    "@paypal/react-paypal-js": "^8.5.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/aos": "^3.0.7",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "eslint-config-react-app": "^7.0.1",
    "git-scm-hooks": "^0.0.6",
    "postcss": "^8.4.39",
    "prettier": "^2.8.8",
    "sass": "^1.67.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.4",
    "typescript": "^4.9.5",
    "verify-commit-msg": "^0.0.11",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-lightningcss": "^0.0.5"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
